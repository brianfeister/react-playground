{
  "name": "exp-ui-core",
  "version": "1.0.0",
  "description": "EXP core admin user interface",
  "main": "./core/index.js",
  "scripts": {
    "start": "webpack-dev-server --env=dev",
    "build": "webpack",
    "build:production": "webpack --env=prod",
    "lint": "eslint main.js core/**/*.js",
    "precommit": "lint-staged",
    "preinstall": "node tasks/preinstall",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "core/**/*.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|public)/"
    ]
  },
  "author": "Brian Feister <brian@drm.st>",
  "license": "N/A",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^20.0.0",
    "lint-staged": "^3.4.1",
    "react-hot-loader": "next",
    "webpack": "2.5.0",
    "webpack-dev-server": "2.4.5"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "repository": "git@github.com:brianfeister/react-playground.git"
}
